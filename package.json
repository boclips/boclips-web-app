    "test": "jest --maxWorkers=50%",
    "test:coverage": "jest --maxWorkers=50% --coverage --collectCoverageFrom='src/**/*.ts*'",
    "test-visual": "percy exec -- ./node_modules/.bin/cypress run",
    "test-visual:open": "start-server-and-test fake http://localhost:9000/ 'percy exec -- ./node_modules/.bin/cypress open'",
    "test-visual:wait": "percy build:wait -f --project f10a8acf/boclips-web-app --commit HEAD",
    "dev": "NODE_ENV=development TAILWIND_MODE=watch webpack serve --config webpack/webpack.dev.js",
    "fake": "NODE_ENV=development TAILWIND_MODE=watch webpack serve --config webpack/webpack.fake.js",
    "compile": "tsc -p . --noEmit",
    "build": "NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "analyse": "NODE_ENV=production webpack --config webpack/webpack.analyse.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint:errors": "npm run lint --",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "all-checks": "npm run lint:errors && npm run compile && npm run test && npm run build"
